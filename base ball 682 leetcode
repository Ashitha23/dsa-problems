class Solution:
    def calPoints(self, operations: List[str]) -> int:
        st=[]
        for ops in operations:
            if ops== '+':
                st.append(st[-1] + st[-2])
            elif ops=='D':
                st.append(2 * st[-1])
            elif ops=='C':
                st.pop()
            else:
                st.append(int(ops))
        return sum(st)
