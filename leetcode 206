class Solution:
    def reverseList(self, head: Optional[ListNode]) -> Optional[ListNode]:
        #iterative approach
        '''prev=None
        curr=head
        while curr:
            nextnode=curr.next
            curr.next=prev
            prev=curr
            curr=nextnode
        return prev'''
#recurssive approach
        if not head or not head.next:
            return head
        newnode=self.reverseList(head.next)
        head.next.next=head
        head.next=None
        return newnode
